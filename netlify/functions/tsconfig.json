{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "moduleResolution": "node",
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": [
            "ES2020"
        ],
        "strict": true,
        "noEmit": true,
        "types": [
            "node",
            "@netlify/functions"
        ]
    },
    "include": [
        "**/*.ts"
    ]
}